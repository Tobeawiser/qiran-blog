<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiran.mapper.ArticleMapper">
    <!--    <resultMap id="BaseResultMap" type="">-->

    <!--    </resultMap>-->

    <select id="listByArticle" resultType="com.qiran.entity.Article">
        select id,
               title,
               article_type,
               short_content,
               content,
               creater,
               create_time,
               updater,
               update_time
        from article
    </select>

</mapper>